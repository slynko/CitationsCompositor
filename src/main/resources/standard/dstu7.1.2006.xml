<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<standard name="dstu7.1.2006" title="ДСТУ ГОСТ 7.1:2006">
    <types>
        <type name="book-author" title="Книги под именем автора (авторов)"/>
        <type name="book-title" title="Книги под заглавием"/>
        <type name="translate" title="Переводное издание"/>
        <type name="helper" title="Справочные издания"/>
        <type name="tutorial" title="Учебники, учебные пособия, лекции"/>
        <type name="methodical" title="Методическая литература"/>
        <type name="collection" title="Сборники научных трудов"/>
        <type name="monograph" title="Монографии"/>
        <type name="article" title="Статьи из сборников, материалов конференций, семинаров, тезисов...."/>
        <type name="magazine-article" title="Статьи из журналов"/>
        <type name="dissertation" title="Диссертации"/>
        <type name="report" title="Отчеты о научно-исследовательских работах"/>
        <type name="shared-science-works" title="Депонированные научные работы"/>
        <type name="patent-documents" title="Патентные документы"/>
        <type name="industrial-catalogs" title="Промышленные каталоги"/>
        <type name="official-documents" title="Официальные документы"/>
        <type name="instructional-regulatory-documents" title="Инструктивно нормативная документация "/>
        <type name="state-standardstandards" title="ГОСТы"/>
        <type name="newspapers-articles" title="Статьи из газет"/>
        <type name="articles-from-the-collected-works" title="Статьи из собрания сочинений "/>
        <type name="component-part-of-the-book" title="Составная часть книги"/>
        <type name="cartographic-publications" title="Картографические издания"/>
        <type name="electronic-resources" title="Электронные ресурсы"/>
        <type name="art-source" title="Изоиздания"/>
    </types>
    <separators>
        <separator name="space">
            <default>&#xA0;</default>
        </separator>
        <separator name="comma">
            <default>,</default>
        </separator>
        <separator name="dot">
            <default>.</default>
        </separator>
        <separator name="semicolon">
            <default>;</default>
        </separator>
        <separator name="colon">
            <default>:</default>
        </separator>
        <separator name="dash">
            <default>–</default>
        </separator>
        <separator name="hyphen">
            <default>-</default>
        </separator>
        <separator name="slash">
            <default>/</default>
        </separator>
        <separator name="dslash">
            <default>//</default>
        </separator>
        <separator name="begin-bracket">
            <default>[</default>
        </separator>
        <separator name="end-bracket">
            <default>]</default>
        </separator>
        <separator name="type-text">
            <default>Text</default>
            <ru>Текст</ru>
            <ua>Текст</ua>
            <en>Text</en>
        </separator>
        <separator name="state-standard">
            <default>ANSI</default>
            <ru>ГОСТ</ru>
            <ua>ДСТУ</ua>
            <en>ANSI</en>
        </separator>
        <separator name="type-art-source">
            <default>Art source</default>
            <ru>Изоматериал</ru>
            <ua>Зобрматеріал</ua>
            <en>Art source</en>
        </separator>
        <separator name="type-maps">
            <default>Maps</default>
            <ru>Карты</ru>
            <ua>Карти</ua>
            <en>Maps</en>
        </separator>
        <separator name="type-resource">
            <default>Electronic resource</default>
            <ru>Электронный ресурс</ru>
            <ua>Електронний ресурс</ua>
            <en>Electronic resource</en>
        </separator>
        <separator name="deposition">
            <default>deposition</default>
            <ru>деп.</ru>
            <ua>деп.</ua>
            <en>deposition</en>
        </separator>
        <separator name="in-force-state">
            <default>in force</default>
            <ru>введ.</ru>
            <ua>чинний</ua>
            <en>in force</en>
        </separator>
        <separator name="others">
            <default>et al.</default>
            <ru>и др.</ru>
            <ua>та ін.</ua>
            <en>et al.</en>
        </separator>
        <separator name="instead-state">
            <default>Instead</default>
            <ru>Взамен</ru>
            <ua>На заміну</ua>
            <en>Instead</en>
        </separator>
        <separator name="access-mode-electronic">
            <default>Access mode</default>
            <ru>Режим доступа</ru>
            <ua>Режим доступу</ua>
            <en>Access mode</en>
        </separator>
        <separator name="director">
            <default>direct.</default>
            <ru>рук.</ru>
            <ua>кер.</ua>
            <en>direct.</en>
        </separator>
        <separator name="executor">
            <default>execut.</default>
            <ru>исполн.</ru>
            <ua>викон.</ua>
            <en>execut.</en>
        </separator>
        <separator name="page">
            <default>p</default>
            <ru>с</ru>
            <ua>с</ua>
            <en>p</en>
        </separator>
        <separator name="pages">
            <default>P</default>
            <ru>С</ru>
            <ua>С</ua>
            <en>P</en>
        </separator>
        <separator name="polyconic-maps">
            <default>polyconic</default>
            <ru>поликон. пр-ция</ru>
            <ua>полікон. пр-ція</ua>
            <en>polyconic</en>
        </separator>
    </separators>
    <multipart-separators-before>
        <multipart-separator name="edition-type" type="default">
            <separator name="begin-bracket"/>
        </multipart-separator>
        <multipart-separator name="type" type="default">
            <separator name="space"/>
            <separator name="colon"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="additional-info" type="default">
            <separator name="space"/>
            <separator name="slash"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="publisher-city" type="default">
            <separator name="dot"/>
            <separator name="space"/>
            <separator name="dash"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="publisher-name" type="default">
            <separator name="dot"/>
            <separator name="space"/>
            <separator name="colon"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="publisher" type="default">
            <separator name="slash"/>
            <separator name="slash"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="year-date" type="default">
            <separator name="comma"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="sectionalizer" type="default">
            <separator name="dot"/>
            <separator name="space"/>
            <separator name="dash"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="pages" type="article">
            <separator name="dot"/>
            <separator name="space"/>
            <separator name="dash"/>
            <separator name="space"/>
            <separator name="pages"/>
            <separator name="dot"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="editors" type="default">
            <separator name="space"/>
            <separator name="semicolon"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="editors" type="book-title">
            <separator name="space"/>
            <separator name="slash"/>
            <separator name="space"/>
        </multipart-separator>
    </multipart-separators-before>
    <multipart-separators-after>
        <multipart-separator name="name" type="default">
            <separator name="dot"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="surname-not-last" type="default">
            <separator name="comma"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="title" type="default">
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="edition-type" type="default">
            <separator name="end-bracket"/>
        </multipart-separator>
        <multipart-separator name="others" type="default">
            <separator name="space"/>
            <separator name="begin-bracket"/>
            <separator name="others"/>
            <separator name="end-bracket"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="others" type="article">
            <separator name="comma"/>
            <separator name="space"/>
            <separator name="others"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="publisher" type="default">
            <separator name="dot"/>
            <separator name="space"/>
        </multipart-separator>
        <multipart-separator name="pages" type="default">
            <separator name="space"/>
            <separator name="page"/>
            <separator name="dot"/>
        </multipart-separator>
        <multipart-separator name="pages" type="article">
            <!--<separator name="hyphen"/>-->
            <separator name="dot"/>
        </multipart-separator>
    </multipart-separators-after>
    <!--TODO think about duplicate authors for the last author's unique separator of surname-->
    <citation-parts>
        <!-- equal to            - "==" - "eq";
        not equal to             - "!=" - "ne";
        greater than             - ">"  - "gt";
        less than                - "<"  - "lt";
        greater than or equal to - ">=" - "gq";
        less than or equal to    - "<=" - "lq" !-->
        <part name="first-author" contentType="authors">
            <authors condition="" type="default">
                <name1>
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2>
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname>
                    <multipart-separator-after name="surname" type="default"/>
                </surname>
            </authors>
        </part>
        <part name="title" contentType="field">
            <field type="default">
                <multipart-separator-after name="title" type="default"/>
            </field>
        </part>
        <part name="edition-type" contentType="field">
            <field type="default">
                <multipart-separator-before name="edition-type" type="default"/>
                <multipart-separator-after name="edition-type" type="default"/>
            </field>
            <field type="electronic-resources">
                <multipart-separator-before name="edition-type" type="electronic-resources"/>
                <multipart-separator-after name="edition-type" type="electronic-resources"/>
            </field>
        </part>
        <part name="type" contentType="field">
            <field type="default">
                <multipart-separator-before name="type" type="default"/>
            </field>
        </part>
        <part name="additional-info" contentType="field">
            <field type="default">
                <multipart-separator-before name="additional-info" type="default"/>
            </field>
        </part>
        <part name="editors" contentType="authors">
            <authors condition="lq4" type="default"> <!-- "<=" - "lq" !-->
                <multipart-separator-before name="editors" type="default"/>
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default">
                    <multipart-separator-after name="surname" type="default"/>
                </surname>
            </authors>
            <authors condition="gt4" type="default"> <!-- ">"  - "gt" !-->
                <multipart-separator-before name="editors" type="default"/>
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default"/>
                <multipart-separator-after name="others" type="default"/>
            </authors>
            <authors condition="lq4" type="book-title"> <!-- "<=" - "lq" !-->
                <multipart-separator-before name="editors" type="book-title"/>
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default">
                    <multipart-separator-after name="surname" type="default"/>
                </surname>
            </authors>
            <authors condition="gt4" type="book-title"> <!-- ">"  - "gt" !-->
                <multipart-separator-before name="editors" type="book-title"/>
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default"/>
                <multipart-separator-after name="others" type="default"/>
            </authors>
        </part>
        <!-- TODO EXAMPLE OF REWRITING OF AUTHORS -->
        <part name="authors-after" contentType="authors">
            <authors condition="lq" count="4" type="default"> <!-- "<=" - "lq" !-->>
                <author>
                    <name1 len="1" type="default">
                        <multipart-separator-after name="name" type="default"/>
                    </name1>
                    <name2 len="1" type="default">
                        <multipart-separator-after name="name" type="default"/>
                    </name2>
                    <surname type="default">
                        <multipart-separator-after name="surname-not-last" type="default"/>
                    </surname>
                </author>
                <author>
                    <name1 len="1" type="default">
                        <multipart-separator-after name="name" type="default"/>
                    </name1>
                    <name2 len="1" type="default">
                        <multipart-separator-after name="name" type="default"/>
                    </name2>
                    <surname type="default">
                        <multipart-separator-after name="surname-not-last" type="default"/>
                    </surname>
                </author>
                <author>
                    <name1 len="1" type="default">
                        <multipart-separator-after name="name" type="default"/>
                    </name1>
                    <name2 len="1" type="default">
                        <multipart-separator-after name="name" type="default"/>
                    </name2>
                    <surname type="default">
                        <multipart-separator-after name="surname-not-last" type="default"/>
                    </surname>
                </author>
                <author>
                    <name1 len="1" type="default">
                        <multipart-separator-after name="name" type="default"/>
                    </name1>
                    <name2 len="1" type="default">
                        <multipart-separator-after name="name" type="default"/>
                    </name2>
                    <surname type="default"/>
                </author>
            </authors>
            <authors condition="gt4" type="default"> <!-- ">" - "gt" !-->>
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default"/>
                <multipart-separator-after name="others" type="default"/>
            </authors>
            <authors condition="gt4" type="article"> <!-- ">" - "gt" !-->
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default">
                    <multipart-separator-after name="surname" type="default"/>
                </surname>
                <multipart-separator-after name="others" type="article"/>
            </authors>
            <authors condition="gt4" type="art-source"> <!-- ">" - "gt" !-->
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default">
                    <multipart-separator-after name="surname" type="default"/>
                </surname>
                <multipart-separator-after name="others" type="default"/>
            </authors>
        </part>
        <part name="directors" contentType="authors">
            <authors condition="lq4" type="report"> <!-- "<=" - "lq" !-->
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default">
                    <multipart-separator-after name="surname" type="default"/>
                </surname>
            </authors>
            <authors condition="gt4" type="report"> <!-- ">" - "gt" !-->
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default"/>
                <multipart-separator-after name="others" type="default"/>
            </authors>
        </part>
        <part name="executors" contentType="authors">
            <authors condition="lq4" type="report"> <!-- "<=" - "lq" !-->
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default">
                    <multipart-separator-after name="surname" type="default"/>
                </surname>
            </authors>
            <authors condition="gt4" type="report"> <!-- ">" - "gt" !-->
                <name1 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name1>
                <name2 type="default">
                    <multipart-separator-after name="name" type="default"/>
                </name2>
                <surname type="default"/>
                <multipart-separator-after name="others" type="default"/>
            </authors>
        </part>
        <part name="publisher-city" contentType="field">
            <field type="default">
                <multipart-separator-before name="publisher-city" type="default"/>
            </field>
        </part>
        <part name="publisher-name" contentType="field">
            <field type="default">
                <multipart-separator-before name="publisher-name" type="default"/>
            </field>
        </part>
        <part name="publisher" contentType="field">
            <field type="default">
                <multipart-separator-before name="publisher" type="default"/>
                <multipart-separator-after name="publisher" type="default"/>
            </field>
        </part>
        <part name="year-date" contentType="field">
            <field type="default">
                <multipart-separator-before name="year-date" type="default"/>
            </field>
        </part>
        <part name="volume" contentType="field">
            <field type="default">
                <multipart-separator-before name="sectionalizer" type="default"/>
            </field>
        </part>
        <part name="no" contentType="field">
            <field type="default">
                <multipart-separator-before name="sectionalizer" type="default"/>
            </field>
        </part>
        <part name="pages" contentType="field">
            <field type="default">
                <multipart-separator-before name="sectionalizer" type="default"/>
                <multipart-separator-after name="pages" type="default"/>
            </field>
            <field type="article">
                <multipart-separator-before name="pages" type="article"/>
                <multipart-separator-after name="pages" type="article"/>
            </field>
        </part>
        <part name="official-date" contentType="field">
            <field type="default">
                <multipart-separator-before name="sectionalizer" type="default"/>
            </field>
        </part>
    </citation-parts>
    <!-- The example how to create citations of different types -->
    <citations>
        <citation type="book-author">
            <string>first-author</string>
            <string>title</string>
            <string>edition-type</string>
            <string>type</string>
            <string>authors-after</string>
            <string>editors</string>
            <string>publisher-city</string>
            <string>publisher-name</string>
            <string>year-date</string>
            <string>pages</string>
        </citation>
        <citation type="book-title">
            <string>title</string>
            <string>edition-type</string>
            <string>type</string>
            <string>editors</string>
            <string>publisher-city</string>
            <string>publisher-name</string>
            <string>year-date</string>
            <string>pages</string>
        </citation>
    </citations>
</standard>
